{
    "configurations": [
        {
            "name": "macOS ARM64",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Core/Source",
                "${workspaceFolder}/Trie/Source",
                "${workspaceFolder}/App/Source",
                "${workspaceFolder}/Vendor/Include",
                "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/include/c++/v1",
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include",
                "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/clang/*/include"
            ],
            "defines": [
                "DEBUG",
                "_DEBUG",
                "PLATFORM_MACOS",
                "__APPLE__",
                "__MACH__"
            ],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-clang-arm64",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/Core/Source",
                    "${workspaceFolder}/Trie/Source",
                    "${workspaceFolder}/App/Source"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            },
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-std=c++20"
            ]
        },
        {
            "name": "macOS Intel",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Core/Source",
                "${workspaceFolder}/Trie/Source",
                "${workspaceFolder}/App/Source",
                "${workspaceFolder}/Vendor/Include",
                "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/include/c++/v1",
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include",
                "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/clang/*/include"
            ],
            "defines": [
                "DEBUG",
                "_DEBUG",
                "PLATFORM_MACOS",
                "__APPLE__",
                "__MACH__"
            ],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-clang-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/Core/Source",
                    "${workspaceFolder}/Trie/Source",
                    "${workspaceFolder}/App/Source"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            },
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-std=c++20"
            ]
        }
    ],
    "version": 4
}
